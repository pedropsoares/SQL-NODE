{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "no-return-await": "off",
        "no-restricted-globals": "off",
        "no-global-assign": "off",
        "no-undef": "off",
        "no-console": "off",
        "no-shadow": "off",
    }
}